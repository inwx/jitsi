<template id = "welcome-page-additional-content-template">
    <div style="text-align: center; font-size: 16px; margin: 20px 0; margin-top: 10px; z-index: 900; position: relative; color: #fff;">  
        {% if jitsi_footer_powered_by_url is defined and jitsi_footer_powered_by_url|length > 0 %}
        <a href="{{ jitsi_footer_powered_by_url }}" style="color: #42b2e2; text-decoration: underline;">{{ jitsi_footer_powered_by_name }}</a> |   
        {% endif %}
        {% if jitsi_footer_powered_dataprotection_url is defined and jitsi_footer_powered_dataprotection_url|length > 0 %}
        <a href="{{ jitsi_footer_powered_dataprotection_url }}" style="color: #42b2e2; text-decoration: underline;">{{ jitsi_footer_powered_dataprotection_text }}</a> |  
        {% endif %}
        {% if jitsi_footer_powered_imprint_url is defined and jitsi_footer_powered_imprint_url|length > 0 %}
        <a href="{{ jitsi_footer_powered_imprint_url }}" style="color: #42b2e2; text-decoration: underline;">{{ jitsi_footer_powered_imprint_text }}</a>  
        {% endif %}
    </div>  
</template>  
