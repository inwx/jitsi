# jitsi-meet coturn config. Do not modify this line
use-auth-secret
keep-address-family
static-auth-secret={{ jitsi_turncredentials_secret }}
realm={{ jitsi_hostname }}
cert={{ jitsi_tls_cert }}
pkey={{ jitsi_tls_privkey }}

no-tcp
listening-port=4446
tls-listening-port=4445
external-ip={{ jitsi_ip }}

syslog
verbose
# jitsi-meet coturn relay disable config. Do not modify this line
no-multicast-peers
no-cli
no-loopback-peers
no-tcp-relay
